{
  "flex-layout.row#testimony2": {
    "props": {
      "blockClass": "testimonyContainer",
      "paddingTop": 7,
      "paddingBottom": 7,
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.col#testimony2colImg",
      "flex-layout.col#testimony2colText"
    ]
  },
  "flex-layout.col#testimony2colImg": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center"
    },
    "children": [
      "image#testimony2"
    ]
  },
  "flex-layout.col#testimony2colText": { 
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center",
      "blockClass": "testimonyTextCol"
    },
    "children": [
      "rich-text#testimonyText2",
      "rich-text#testimonyAuthor2"
    ]
  },
  "image#testimony2": {
    "props": {
      "src": "assets/testimonyPlaceholder.png",
      "blockClass": "testimonyImg"
    }
  },
  "rich-text#testimonyText2": {
    "props": {
      "blockClass": "testimonyBody",
      "textAlignment": "CENTER",
      "text": "“Apps have saved us a tremendous amount of time and money.”"
    }
  },
  "rich-text#testimonyAuthor2": { 
    "props": {
      "blockClass": "testimonyAuthor",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "— Morgan Haynes, COO at Karm"
    }
  }
}