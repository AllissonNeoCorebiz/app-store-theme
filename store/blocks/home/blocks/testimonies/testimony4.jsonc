{
  "flex-layout.row#testimony4": {
    "props": {
      "blockClass": "testimonyContainer",
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.col#testimony4colImg",
      "flex-layout.col#testimony4colText"
    ]
  },
  "flex-layout.col#testimony4colImg": {
    "props": {
      "blockClass": "testimonyCol",
      "verticalAlign": "middle",
      "horizontalAlign": "center"
    },
    "children": [
      "image#testimony4"
    ]
  },
  "flex-layout.col#testimony4colText": { 
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center",
      "blockClass": ["testimonyTextCol", "testimonyCol"]
    },
    "children": [
      "rich-text#testimonyText4",
      "rich-text#testimonyAuthor4"
    ]
  },
  "image#testimony4": {
    "props": {
      "width": 120,
      "height": "auto",
      "src": "assets/testimonyPlaceholder.png",
      "blockClass": "testimonyImg"
    }
  },
  "rich-text#testimonyText4": {
    "props": {
      "blockClass": "testimonyBody",
      "textAlignment": "CENTER",
      "text": "“Apps have saved us a tremendous amount of time and money.”",
      "font": { 
        "desktop": "t-heading-5",
        "mobile": "t-heading-5"
      }
    }
  },
  "rich-text#testimonyAuthor4": { 
    "props": {
      "blockClass": "testimonyAuthor",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-6",
      "text": "— Morgan Haynes, COO at Karm"
    }
  }
}