{
  "responsive-layout.phone#bottomCards": { 
    "children": [
      "flex-layout.row#bottomCardsMobile"
    ]
  },
  "flex-layout.row#bottomCardsMobile": { 
    "props": { 
      "marginTop": 8,
      "marginBottom": 8
    },
    "children": [ 
      "flex-layout.col#bottomCardsMobile"
    ]
  },
  "flex-layout.col#bottomCardsMobile": {
    "children": [ 
      "rich-text#bottomCardsTitle",
      "flex-layout.row#bottomCardsBodyMobile"
    ]
  },
  "flex-layout.row#bottomCardsBodyMobile": {
    "props": { 
      "marginTop": 6
    },
    "children": [ 
      "flex-layout.col#bottomCardsBodyMobile"
    ]
  },
  "flex-layout.col#bottomCardsBodyMobile": {
    "props": { 
      "rowGap": 6
    },
    "children": [ 
      "stack-layout#bottomCard1Mobile",
      "stack-layout#bottomCard2Mobile",
      "stack-layout#bottomCard3Mobile"
    ]
  },
  "stack-layout#bottomCard1Mobile": {
    "props": {
      "blockClass": "bottomCard1Mobile"
    },
    "children": [
      "image#bottomCard1Mobile",
      "rich-text#bottomCardsText1Mobile"
    ]
  },
  "stack-layout#bottomCard2Mobile": { 
    "props": {
      "blockClass": "bottomCard1Mobile"
    },
    "children": [
      "image#bottomCard2Mobile",
      "rich-text#bottomCardsText2Mobile"
    ]
  },
  "stack-layout#bottomCard3Mobile": { 
    "props": {
      "blockClass": "bottomCard1Mobile"
    },
    "children": [
      "image#bottomCard3Mobile",
      "rich-text#bottomCardsText3Mobile"
    ]
  },
  "image#bottomCard1Mobile": {
    "props": {
      "width": "100%",
      "height": "8rem",
      "src": "assets/bottomMobile.png"
    }
  },
  "image#bottomCard2Mobile": {
    "props": {
      "width": "100%",
      "height": "8rem",
      "src": "assets/isaMobile.png"
    }
  },
  "image#bottomCard3Mobile": {
    "props": {
      "width": "100%",
      "height": "8rem",
      "src": "assets/brenoMobile.png"
    }
  },



  /**

  DESKTOP LAYOUT 

  */
  "responsive-layout.desktop#bottomCards": { 
    "children": [
      "flex-layout.row#bottomCards"
    ]
  },
  "responsive-layout.tablet#bottomCards": { 
    "children": [
      "flex-layout.row#bottomCards"
    ]
  },
  "flex-layout.row#bottomCards": {
    "props": { 
      "marginTop": 10,
      "marginBottom": 10
    },
    "children": [
      "flex-layout.col#bottomCards"      
    ]      
  },
  "flex-layout.col#bottomCards": {
    "props": { 
      "rowGap": 9
    },
    "children": [
      "rich-text#bottomCardsTitle",
      "flex-layout.row#bottomCardsBody"
    ]
  },
  "flex-layout.row#bottomCardsBody": {
    "props": {
      "colGap": 8,
      "preventHorizontalStretch": true
    },
    "children": [
      "stack-layout#bottomCard1",
      "stack-layout#bottomCard2",
      "stack-layout#bottomCard3"
    ]
  },
  "stack-layout#bottomCard1": {
    "children": [
      "image#bottomCard1",
      "flex-layout.col#bottomCardsCard1"
    ]
  },
  "stack-layout#bottomCard2": {
    "children": [
      "image#bottomCard2",
      "flex-layout.col#bottomCardsCard2"
    ]
  },
  "stack-layout#bottomCard3": {
    "children": [
      "image#bottomCard3",
      "flex-layout.col#bottomCardsCard3"
    ]
  },
  "image#bottomCard1": {
    "props": {
      "height": "auto",
      "width": "auto",
      "src": "assets/bottomCard1.png",
      "blockClass": "bottomCards"
    }
  },
  "image#bottomCard2": {
    "props": {
      "height": "auto",
      "width": "auto",
      "src": "assets/bottomCard2.png",
      "blockClass": "bottomCards"
    }
  },
  "image#bottomCard3": {
    "props": {
      "height": "auto",
      "width": "auto",
      "src": "assets/bottomCard3.png",
      "blockClass": "bottomCards"
    }
  },
  "flex-layout.col#bottomCardsCard1": {
    "props": {
      "blockClass": "bottomCardsCol",
      "paddingLeft": 6,
      "paddingRight": 6,
      "verticalAlign": "bottom"
    },
    "children": [
      "rich-text#bottomCardsText1",
      "link#bottomCardsLink1"
    ]
  },
  "flex-layout.col#bottomCardsCard2": {
    "props": {
      "blockClass": "bottomCardsCol",
      "paddingLeft": 6,
      "paddingRight": 6,
      "verticalAlign": "bottom"
    },
    "children": [
      "rich-text#bottomCardsText2",
      "link#bottomCardsLink2"
    ]
  },
  "flex-layout.col#bottomCardsCard3": {
    "props": {
      "blockClass": "bottomCardsCol",
      "paddingLeft": 6,
      "paddingRight": 6,
      "verticalAlign": "bottom"
    },
    "children": [
      "rich-text#bottomCardsText3",
      "link#bottomCardsLink3"
    ]
  },
