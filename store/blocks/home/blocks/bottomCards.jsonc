  "rich-text#bottomCardsText2Mobile": {
    "props": {
      "text": "Find the best\n[solutions to grow]()\nyour business.",
      "font": { 
        "phone": "t-heading-5"
      },
      "blockClass": ["bottomCardsBodyMobile", "noMargin", "leftPositioned"]
    }
  },
  "rich-text#bottomCardsText3Mobile": {
    "props": {
      "text": "[Develop apps]()\n for global\nclients.",
      "font": { 
        "mobile": "t-heading-5"
      },
