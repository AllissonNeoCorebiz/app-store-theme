{
  "store.custom#all-apps": {
    "blocks": [
      "flex-layout.row#search-bar",
      "flex-layout.row#allapps-container"
    ]
  },
  "flex-layout.row#allapps-container": {
    "props": {
      "blockClass": "allAppsContainer",
      "paddingBottom": 10
    },
    "children": ["flex-layout.col#allapps-container"]
  },
  "flex-layout.col#allapps-container": {
    "children": [
      "flex-layout.row#banners",
      "flex-layout.row#mainHighlights",
      "flex-layout.row#marketing",
      "flex-layout.row#review",
      "flex-layout.row#customerSupport",
      "flex-layout.row#channels",
      "flex-layout.row#finance",
      "flex-layout.row#omnichannel",
      "flex-layout.row#privacy",
      "flex-layout.row#developer",
      "responsive-layout.desktop#all-apps",
      "responsive-layout.tablet#all-apps",
      "responsive-layout.phone#all-apps"
    ]
  },
  "flex-layout.row#banners": {
    "props": {
      "marginTop": 9,
      "marginBottom": 8
    },
    "children": ["list-context.image-list#demo"]
  },
  "list-context.image-list#demo": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 720,
      "images": [{
        "image": "https://i.imgur.com/irYHIAJ.png",
        "mobileImage": "https://i.imgur.com/irYHIAJ.png"
      }]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  }
}